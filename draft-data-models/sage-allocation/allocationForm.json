  "allOf": [
    {
      "if": {
        "properties": {
          "category": {
            "properties": {
              "projectCategory": {
                "const": "Other"
              }
            }
          }
        }
      },
      "then": {
        "properties": {
          "category": {
            "properties": {
              "otherProjectCategory": {
                "type": "string",
                "title": "Please specify the type of project"
              }
            }
          }
        }
      }
    },
    {
      "if": {
        "properties": {
          "dataProcessingModule": {
