{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/SizeTable",
  "title": "SizeTable",
  "type": "object",
  "properties": {
    "sizeName": {
      "enum": [
        "xsmall",
        "medium",
        "small",
        "large",
        "xlarge"
      ]
    },
    "size_PK": {
      "not": {
        "type": "null"
      },
      "minLength": 1
    }
  },
  "required": [
    "sizeName",
    "size_PK"
  ]
}